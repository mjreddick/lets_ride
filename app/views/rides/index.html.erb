<div class='container'>
	<div class='row'>
		<div class='col-md-6 col-md-4'>
			<div class='panel panel-success'>
				<div class='panel-heading'>
					<h2 class='inline text-center'>Rides</h2>
				</div>
				<div class='panel-body'>
					<% @users.each do |user| %> 
						<div class='list-group'>
							<h4>Ride event</h4>
						</div>
						<div class='l'>
							
						</div>
          <%= user.first_name %>
          <%= image_tag(user.avatar.url(:thumb), size: "100x100") %>
					<% end %>				
				</div>
			</div>
		</div>
	</div>
</div>
